<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
    <Appenders>
        <File name="file_appender" filename="logs\file_appender.log">
            <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} %p %m%n" />
        </File>
        <Console name="file_appender1" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} %p %m%n" />
        </Console>
    </Appenders>
    <Loggers>
        <Root level="INFO">
            <appender-ref ref="file_appender"/>
            <appender-ref ref="file_appender1"/>
        </Root>
    </Loggers>
</Configuration>